<?xml version="1.0" encoding="utf-8"?>
<com.github.florent37.materialviewpager.MaterialViewPager xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/materialViewPager"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:viewpager_animatedHeaderImage="true"
    app:viewpager_color="@color/colorPrimary"
    app:viewpager_displayToolbarWhenSwipe="true"
    app:viewpager_enableToolbarElevation="true"
    app:viewpager_headerAdditionalHeight="20dp"
    app:viewpager_headerAlpha="1.0"
    app:viewpager_headerHeight="200dp"
    app:viewpager_hideToolbarAndTitle="true"
    app:viewpager_imageHeaderDarkLayerAlpha="0.0"
    app:viewpager_pagerTitleStrip="@layout/material_view_pager_pagertitlestrip_standard"
    app:viewpager_parallaxHeaderFactor="1.5"
    app:viewpager_transparentToolbar="false"
    tools:context="com.dat.barnaulzoopark.ui.animalsdetail.AnimalsDetailActivity" />